{
  "name": "interactivesolutions/honeycomb-companies",
  "description": "Companies package",
  "keywords": [
    "honeycomb",
    "laravel",
    "package",
    "companies"
  ],
  "homepage": "https://github.com/interactivesolutions/honeycomb-companies",
  "license": "MIT",
  "authors": [
    {
      "name": "Jevgenij Dmitrijev",
      "email": "jd@interactivesolutions.lt"
    },
    {
      "name": "Mantas Paulauskas",
      "email": "mantopastas@gmail.com"
    }
  ],
  "repositories": [],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "^5.5",
    "interactivesolutions/honeycomb-core": "~1.1.0",
    "interactivesolutions/honeycomb-regions": "^1.1",
    "interactivesolutions/honeycomb-acl": "^1.1"
  },
  "require-dev": {
    "brianium/paratest": "^1.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench-browser-kit": "^3.5",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "InteractiveSolutions\\HoneycombCompanies\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "paratest -p4 --colors"
    ]
  },
  "config": {
    "secure-http": false,
    "platform": {
      "php": "7.1"
    }
  }
}
